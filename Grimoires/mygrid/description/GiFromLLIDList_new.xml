<?xml version="1.0" encoding="UTF-8"?>
<serviceDescriptions xmlns="http://www.mygrid.org.uk/feta/">
	<serviceDescription>
		<serviceName>SHoundGiFromLLIDList</serviceName>
		<organisation>
			<organisationName>Blueprint</organisationName>
			<organisationDescriptionText>The Blueprint Initiative</organisationDescriptionText>
		</organisation>

		<locationURL>http://seqhound.blueprint.org/SHoundGiFromLLIDList</locationURL>
		<interfaceWSDL>http://seqhound.blueprint.org/SHoundGiFromLLIDList?wsdl</interfaceWSDL>
		<serviceDescriptionText>get the protein gi's associated with the locuslink id</serviceDescriptionText>
		<operations>
			<serviceOperation>
				<operationName>SHoundGiFromLLIDList</operationName>
				<operationInputs>
					<parameter>
						<parameterName>llidList</parameterName>
						<semanticType>http://www.mygrid.org.uk/ontology#LocusLink_record_id</semanticType>
						<transportDataType>String</transportDataType>
					</parameter>

				</operationInputs>

				<operationOutputs>
					<parameter>
						<parameterName>result</parameterName>
						<semanticType>http://www.mygrid.org.uk/ontology#GenBank_gene_id_%28GI%29</semanticType>
						<transportDataType>String</transportDataType>
					</parameter>

				</operationOutputs>

				<operationTask>http://www.mygrid.org.uk/ontology#retrieving</operationTask>
				<operationResource>http://www.mygrid.org.uk/ontology#bioinformatics_database</operationResource>
			</serviceOperation>

		</operations>

		<serviceType>SeqHound service</serviceType>
	</serviceDescription>

</serviceDescriptions>