<?xml version="1.0" encoding="UTF-8"?>
<serviceDescriptions xmlns="http://www.mygrid.org.uk/feta/">
	<serviceDescription>
		<serviceName>SHoundGetReferenceIDFromGiList</serviceName>
		<organisation>
			<organisationName>Blueprint</organisationName>
			<organisationDescriptionText>The Blueprint Initiative</organisationDescriptionText>
		</organisation>

		<locationURL>http://seqhound.blueprint.org/SHoundGetReferenceIDFromGiList</locationURL>
		<interfaceWSDL>http://seqhound.blueprint.org/SHoundGetReferenceIDFromGiList?wsdl</interfaceWSDL>
		<serviceDescriptionText>Retrieves the MUID or PMID for a given gi.</serviceDescriptionText>
		<operations>
			<serviceOperation>
				<operationName>SHoundGetReferenceIDFromGiList</operationName>
				<operationInputs>
					<parameter>
						<parameterName>giList</parameterName>
						<semanticType>http://www.mygrid.org.uk/ontology#Gene_Ontology_term_id</semanticType>
						<transportDataType>String</transportDataType>
						<collectionSemanticType>Ordered Collection</collectionSemanticType>
					</parameter>

					<parameter>
						<parameterName>reftype</parameterName>
						<semanticType>http://www.mygrid.org.uk/ontology#bioinformatics_identifier</semanticType>
						<transportDataType>String</transportDataType>
						<collectionSemanticType>Ordered Collection</collectionSemanticType>
					</parameter>

				</operationInputs>

				<operationOutputs>
					<parameter>
						<parameterName>result</parameterName>
						<semanticType>http://www.mygrid.org.uk/ontology#bioinformatics_identifier</semanticType>
						<transportDataType>String</transportDataType>
						<collectionSemanticType>Ordered Collection</collectionSemanticType>
					</parameter>

				</operationOutputs>

				<operationTask>http://www.mygrid.org.uk/ontology#retrieving</operationTask>
				<operationResource>http://www.mygrid.org.uk/ontology#bioinformatics_database</operationResource>
				<operationResourceContent>http://www.mygrid.org.uk/ontology#model_organism</operationResourceContent>
				<operationMethod>http://www.mygrid.org.uk/ontology#bioinformatics_algorithm</operationMethod>
				<operationApplication>http://www.mygrid.org.uk/ontology#bioinformatics_application</operationApplication>
			</serviceOperation>

		</operations>

		<serviceType>SeqHound service</serviceType>
	</serviceDescription>

</serviceDescriptions>