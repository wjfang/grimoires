<?xml version="1.0" encoding="UTF-8"?>
<serviceDescriptions xmlns="http://www.mygrid.org.uk/feta/">
	<serviceDescription>
		<serviceName>SHoundGODBGetParentOf</serviceName>
		<organisation>
			<organisationName>Blueprint</organisationName>
			<organisationDescriptionText>The Blueprint Initiative</organisationDescriptionText>
		</organisation>

		<locationURL>http://seqhound.blueprint.org/SHoundGODBGetParentOf</locationURL>
		<interfaceWSDL>http://seqhound.blueprint.org/SHoundGODBGetParentOf?wsdl</interfaceWSDL>
		<serviceDescriptionText>retrieves an array of the parents of a given goid</serviceDescriptionText>
		<operations>
			<serviceOperation>
				<operationName>SHoundGODBGetParentOf</operationName>
				<operationInputs>
					<parameter>
						<parameterName>goid</parameterName>
						<semanticType>http://www.mygrid.org.uk/ontology#Gene_Ontology_term_id</semanticType>
						<transportDataType>String</transportDataType>
						<collectionSemanticType>SetSingle</collectionSemanticType>
					</parameter>

				</operationInputs>

				<operationOutputs>
					<parameter>
						<parameterName>result</parameterName>
						<semanticType>http://www.mygrid.org.uk/ontology#bioinformatics_identifier</semanticType>
						<transportDataType>String</transportDataType>
						<collectionSemanticType>Set</collectionSemanticType>
					</parameter>

				</operationOutputs>

				<operationTask>http://www.mygrid.org.uk/ontology#retrieving</operationTask>
				<operationResource>http://www.mygrid.org.uk/ontology#Genbank_nucleotide_sequence_database</operationResource>
				<operationResourceContent>http://www.mygrid.org.uk/ontology#model_organism</operationResourceContent>
				<operationMethod>http://www.mygrid.org.uk/ontology#bioinformatics_algorithm</operationMethod>
				<operationApplication>http://www.mygrid.org.uk/ontology#bioinformatics_application</operationApplication>
			</serviceOperation>

		</operations>

		<serviceType>SeqHound service</serviceType>
	</serviceDescription>

</serviceDescriptions>